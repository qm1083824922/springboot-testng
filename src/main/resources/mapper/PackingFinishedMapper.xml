<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sailheader.testng.mapper.PackingFinishedMapper">

    <select id="selectByBarcode" resultType="com.sailheader.testng.entity.PackingFinished">
        select * from wh_packing_finished
        where sequence_number = #{barcode}
    </select>
</mapper>